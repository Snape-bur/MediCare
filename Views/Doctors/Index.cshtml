@model IEnumerable<MediCare.Models.Doctor>

@{
    ViewData["Title"] = "Index";
}

<h1>Index</h1>

<p>
    <a asp-action="Create">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.AppUser)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Specialty)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.ConsultationFee)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.AvailabilitySchedule)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.ProfileInfo)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.AppUser.Id)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Specialty.SpecialtyId)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.ConsultationFee)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.AvailabilitySchedule)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.ProfileInfo)
            </td>
            <td>
                <a asp-action="Edit" asp-route-id="@item.DoctorId">Edit</a> |
                <a asp-action="Details" asp-route-id="@item.DoctorId">Details</a> |
                <a asp-action="Delete" asp-route-id="@item.DoctorId">Delete</a>
            </td>
        </tr>
}
    </tbody>
</table>
